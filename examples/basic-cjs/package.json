{
  "name": "express-numflow-basic-cjs-example",
  "version": "1.0.0",
  "description": "Basic CJS example for express-numflow with module-alias",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "FEATURE_LOGS=true nodemon app.js"
  },
  "_moduleAliases": {
