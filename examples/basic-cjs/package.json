    "express-numflow": "^0.1.8",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "express-numflow",
    "commonjs",
    "example"
  ],
  "license": "MIT"
}
